{
  "name": "gama_asset_client",
  "main": "./index.html",
  "nodejs": true,
  "node-main":"./js/index.js",
  "description": "Gama 网站提供的桌面API服务",
  "version": "0.1.1",
  "keywords": ["node", "gama", "webkit", "desktop"],
  "homepage": "https://github.com/GamaLabs/gama-asset-client",
  "author":{
    "name": "GamaLabs",
    "email": ""
  },
  "dependencies":{
    "underscore": "1.5.2",
    "shelljs": "latest",
    "oauth-sign": "0.4.0",
    "async": "0.2.10",
    "wgetjs": "0.3.3"
  },
  "window": {
    "title": "Gama 桌面API服务",
    "icon": "link.png",
    "debug": true,
    "toolbar": true,
    "resizable":true,
    "position": "center",
    "frame":true,
    "fullscreen": false,
    "width":1280,
    "height":960,
    "min_width":1024,
    "min_height":640
  },
  "webkit":{
    "plugin": true
  },
  "chromium-args":"--enable-threaded-compositing"
}
